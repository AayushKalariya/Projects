public class Practice {
	
	 public static void main(String[] args) {
		int[][] arr = {{1,2,3},
				       {2,9,4},
				       {3,8,3}};
		int sum = 0;
		
         for(int i = 0; i < arr[0].length;i++) {
        	 for(int j = 0; j < arr.length;j++) {
        		sum += arr[j][i];
        	 }
         }
       System.out.println(sum);
		
		
		}
		
	 }
